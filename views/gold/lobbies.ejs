<html>
    <head>
        <title><%= title %></title>
        <script></script>
    </head>
    <body>
        <div id = "nav"></div>
        <div id = "games">
        <ul>
            <% data.forEach(function(lobby){ %>
                <li>
                    <a href="/gold/play?code=<%= lobby.code %>">
                        <%= lobby.gamename %> - <%= lobby.hostname %> [<%= lobby.players %> / <%= lobby.maxplayers %>]
                    </a>
                </li>
            <% }) %>
        </ul>
        </div>
    </body>
</html>